{
  "name": "job-cart",
  "version": "0.1.0",
  "description": "Job~Ad Cart Assestment",
  "main": "index.js",
  "scripts": {
    "start": "serve dist -s -c 1 -p 8082",
    "prestart": "npm run build",
    "dev": "webpack-dev-server --mode development --inline --hot --progress --cert",
    "build": "webpack --mode production --progress",
    "clean": "npm cache clean --force && rimraf -- node_modules dist",
    "stop": "killall node",
    "test": "npm run -s lint && jest --coverage -u",
    "test:watch": "npm run -s -u test --watch",
    "lint": "eslint src test"
  },
  "keywords": [
    "NodeJS",
    "React",
    "Webpack"
  ],
  "author": "Riky Liang",
  "license": "ISC",
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "enzyme": "^3.6.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "chai-redux": "^1.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "eslint": "^5.5.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-react": "^7.11.1",
    "expect": "^23.6.0",
    "file-loader": "^2.0.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-environment-jsdom": "^23.4.0",
    "json-loader": "^0.5.7",
    "jsx-chai": "^4.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "promise-polyfill": "^8.1.0",
    "raw-loader": "^0.5.1",
    "react-slick": "^0.23.1",
    "react-test-renderer": "^16.5.2",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "serve": "^10.0.0",
    "sinon": "^6.3.4",
    "style-loader": "^0.23.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.1.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-md5-hash": "0.0.6"
  }
}
